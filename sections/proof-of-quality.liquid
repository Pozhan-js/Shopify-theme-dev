  color: #333;
  text-transform: uppercase;
}

.cert-content {
  flex: 1;
  padding: 16px 0;
}

.cert-lines {
  height: 2px;
  background: #eee;
  margin: 8px 0;
  border-radius: 1px;
}

.cert-lines:nth-child(1) {
  width: 80%;
}

.cert-lines:nth-child(2) {
  width: 100%;
}

.cert-lines:nth-child(3) {
  width: 60%;
}

.cert-standard {
  font-size: 8px;
  font-weight: bold;
  color: #C22E35;
  text-align: center;
  margin-top: 12px;
  padding: 4px;
  border: 1px solid #C22E35;
  border-radius: 4px;
}

.cert-footer {
  border-top: 1px solid #eee;
  padding-top: 8px;
}

.cert-seals {
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.cert-seal {
  width: 24px;
  height: 24px;
  border-radius: 50%;
  background: linear-gradient(45deg, #C22E35, #A02329);
  opacity: 0.7;
}

.download-icon {
  position: absolute;
  bottom: 16px;
  right: 16px;
  width: 32px;
  height: 32px;
  background: rgba(194, 46, 53, 0.9);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #ffffff;
  cursor: pointer;
  transition: all 0.3s ease;
  opacity: 0;
}

.certificate-display:hover .download-icon {
  opacity: 1;
}

.download-icon:hover {
  background: #A02329;
  transform: scale(1.1);
}

.download-icon svg {
  width: 16px;
  height: 16px;
}

.quality-info {
  background: #C22E35;
  padding: 24px 20px;
  color: #ffffff;
  text-align: left;
}

.quality-cert-title {
  font-size: 24px;
  font-weight: 600;
  color: #ffffff;
  margin: 0 0 12px 0;
  line-height: 1.3;
}

.quality-description {
  font-size: 14px;
  font-weight: 400;
  color: rgba(255, 255, 255, 0.9);
  margin: 0;
  line-height: 1.5;
}

