{% schema %}
{
  "name": "Proof of Quality",
  "tag": "section",
  "class": "proof-of-quality-section",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Section Title",
      "default": "Proof of Quality"
    },
    {
      "type": "textarea",
      "id": "subtitle",
      "label": "Section Subtitle",
      "default": "Every XS Pilates product is manufactured under internationally recognized standards for quality, safety, and sustainability."
    },
    {
      "type": "header",
      "content": "FSCÂ® Certification"
    },
    {
      "type": "image_picker",
      "id": "fsc_certificate",
      "label": "FSC Certificate Image",
      "info": "Upload FSC certification document (recommended size: 320x400px)"
    },
    {
      "type": "text",
