<div class="wwo-section">
  <div class="wwo-container">
    <div class="wwo-header">
      <h2 class="wwo-title">
        {{ section.settings.title | default: "What We Offer" }}
      </h2>
    </div>
    
    <div class="wwo-content">
      <div class="wwo-left">
        <div class="wwo-description">
          {{ section.settings.description | default: "We design and build studio-grade apparatus with over 30+ handcrafted steps, eco-minded finishes, and rigorous QC. From single prototypes to full private-label runs, XS Pilates delivers the quality your brand stands for." }}
        </div>
        {% if section.settings.button_text != blank %}
          <div class="wwo-cta">
            <a href="{{ section.settings.button_url | default: '#' }}" class="wwo-btn wwo-btn-consultation">
              {% if section.settings.button_icon != blank %}
                <img src="{{ section.settings.button_icon | img_url: '24x24' }}" alt="Icon" class="wwo-btn-icon">
              {% else %}
                <span class="wwo-btn-arrow">></span>
              {% endif %}
              {{ section.settings.button_text }}
            </a>
          </div>
        {% endif %}
      </div>
      
      <div class="wwo-right">
        <div class="wwo-cards">
          <div class="wwo-card wwo-manufacturing-card">
            {% if section.settings.manufacturing_image != blank %}
              <img src="{{ section.settings.manufacturing_image | img_url: '680x400' }}" alt="Full-Line Manufacturing" class="wwo-card-image">
            {% else %}
              <div class="wwo-card-image-placeholder wwo-manufacturing-bg"></div>
            {% endif %}
            <div class="wwo-card-content">
              <h3 class="wwo-card-title">
                {{ section.settings.manufacturing_title | default: "Full-Line Manufacturing" }}
              </h3>
            </div>
          </div>
          
          <div class="wwo-card wwo-solutions-card">
            {% if section.settings.solutions_image != blank %}
              <img src="{{ section.settings.solutions_image | img_url: '680x400' }}" alt="Comprehensive OEM Solutions" class="wwo-card-image">
            {% else %}
              <div class="wwo-card-image-placeholder wwo-solutions-bg"></div>
            {% endif %}
            <div class="wwo-card-content">
              <h3 class="wwo-card-title">
                {{ section.settings.solutions_title | default: "Comprehensive OEM Solutions" }}
              </h3>
              <ul class="wwo-card-features">
                {% for block in section.blocks %}
                  {% if block.type == 'feature' %}
                    <li class="wwo-feature-item">
                      {{ block.settings.feature_text }}
                    </li>
                  {% endif %}
                {% endfor %}
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

