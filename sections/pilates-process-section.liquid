<div class="pilates-process-section">
  <div class="pilates-process-container">
    <div class="pilates-process-header">
      <h2 class="pilates-process-title">
        {{ section.settings.title | default: "Your Partner in Pilates & Fitness Solutions (Process)" }}
      </h2>
    </div>
    
    <div class="pilates-process-grid">
      <!-- Step 1: Consultation -->
      <div class="pilates-process-card pilates-process-card-1">
        <div class="pilates-process-image">
          {% if section.settings.consultation_image != blank %}
            <img src="{{ section.settings.consultation_image | img_url: '460x287' }}" alt="Consultation" class="pilates-process-img">
          {% else %}
            <img src="https://images.unsplash.com/photo-1560472354-b33ff0c44a43?ixlib=rb-4.0.3&auto=format&fit=crop&w=460&q=80" alt="Consultation" class="pilates-process-img">
          {% endif %}
        </div>
        <div class="pilates-process-content">
          <h3 class="pilates-process-step-title">
            {{ section.settings.consultation_title | default: "Consultation" }}
          </h3>
          <p class="pilates-process-step-description">
            {{ section.settings.consultation_description | default: "Share your concept, specifications, or drawings. Our team advises on materials, functions, and feasibility." }}
          </p>
          <div class="pilates-process-step-number">1</div>
        </div>
      </div>
      
      <!-- Step 2: Sample & Quote -->
      <div class="pilates-process-card pilates-process-card-2">
        <div class="pilates-process-image">
          {% if section.settings.sample_image != blank %}
            <img src="{{ section.settings.sample_image | img_url: '460x287' }}" alt="Sample & Quote" class="pilates-process-img">
          {% else %}
            <img src="https://images.unsplash.com/photo-1581091226825-a6a2a5aee158?ixlib=rb-4.0.3&auto=format&fit=crop&w=460&q=80" alt="Sample & Quote" class="pilates-process-img">
          {% endif %}
        </div>
        <div class="pilates-process-content">
          <h3 class="pilates-process-step-title">
            {{ section.settings.sample_title | default: "Sample & Quote" }}
          </h3>
          <p class="pilates-process-step-description">
            {{ section.settings.sample_description | default: "We create samples or prototypes for your approval. Transparent pricing, fast sample lead time." }}
          </p>
          <div class="pilates-process-step-number">2</div>
        </div>
      </div>
      
      <!-- Step 3: Production -->
      <div class="pilates-process-card pilates-process-card-3">
        <div class="pilates-process-image">
          {% if section.settings.production_image != blank %}
            <img src="{{ section.settings.production_image | img_url: '460x600' }}" alt="Production" class="pilates-process-img">
          {% else %}
            <img src="https://images.unsplash.com/photo-1581092795360-fd1ca04f0952?ixlib=rb-4.0.3&auto=format&fit=crop&w=460&q=80" alt="Production" class="pilates-process-img">
          {% endif %}
        </div>
        <div class="pilates-process-content">
          <h3 class="pilates-process-step-title">
            {{ section.settings.production_title | default: "Production" }}
          </h3>
          <p class="pilates-process-step-description">
            {{ section.settings.production_description | default: "Mass production in our 150,000sqm facility by our skilled production team, with strict quality control at every stage." }}
          </p>
          <div class="pilates-process-step-number">3</div>
        </div>
      </div>
      
      <!-- Step 4: Packaging & Delivery -->
      <div class="pilates-process-card pilates-process-card-4">
        <div class="pilates-process-image">
          {% if section.settings.packaging_image != blank %}
            <img src="{{ section.settings.packaging_image | img_url: '460x287' }}" alt="Packaging & Delivery" class="pilates-process-img">
          {% else %}
            <img src="https://images.unsplash.com/photo-1586528116311-ad8dd3c8310d?ixlib=rb-4.0.3&auto=format&fit=crop&w=460&q=80" alt="Packaging & Delivery" class="pilates-process-img">
          {% endif %}
        </div>
        <div class="pilates-process-content">
          <h3 class="pilates-process-step-title">
            {{ section.settings.packaging_title | default: "Packaging & Delivery" }}
          </h3>
          <p class="pilates-process-step-description">
            {{ section.settings.packaging_description | default: "Protective, custom packaging options. Support for sea and air shipment worldwide." }}
          </p>
          <div class="pilates-process-step-number">4</div>
        </div>
      </div>
      
      <!-- Step 5: After-sales Support -->
      <div class="pilates-process-card pilates-process-card-5">
        <div class="pilates-process-image">
          {% if section.settings.support_image != blank %}
            <img src="{{ section.settings.support_image | img_url: '460x287' }}" alt="After-sales Support" class="pilates-process-img">
          {% else %}
            <img src="https://images.unsplash.com/photo-1559136555-9303baea8ebd?ixlib=rb-4.0.3&auto=format&fit=crop&w=460&q=80" alt="After-sales Support" class="pilates-process-img">
          {% endif %}
        </div>
        <div class="pilates-process-content">
          <h3 class="pilates-process-step-title">
            {{ section.settings.support_title | default: "After-sales Support" }}
          </h3>
          <p class="pilates-process-step-description">
            {{ section.settings.support_description | default: "Reliable warranty, spare parts supply, and technical assistance after delivery." }}
          </p>
          <div class="pilates-process-step-number">5</div>
        </div>
      </div>
    </div>
  </div>
</div>

<style>
.pilates-process-section {
  padding: 10px 0;
  background-color: #ffffff;
}

.pilates-process-container {
  max-width: 1520px;
  margin: 0 auto;
  padding: 0 20px;
}

.pilates-process-header {
  text-align: center;
