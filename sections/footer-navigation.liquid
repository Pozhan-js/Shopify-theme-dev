                  {%- else -%}
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="none">
                      <circle cx="12" cy="12" r="10" stroke="currentColor" stroke-width="2"/>
                      <path d="M8 12h8M12 8v8" stroke="currentColor" stroke-width="2"/>
                    </svg>
                {%- endcase -%}
                <span class="social-text">{{ block.settings.social_text }}</span>
              </a>
            {%- endif -%}
          {%- endfor -%}
        </div>
      </div>
    </div>

    <!-- 版权信息 -->
    {%- if section.settings.show_copyright -%}
      <div class="footer-copyright">
        <p class="copyright-text">
          {{ section.settings.copyright_text | default: '© 2024 All rights reserved.' }}
        </p>
      </div>
    {%- endif -%}
  </div>
</footer>

<style>
  .footer-navigation-container {
    background: {{ section.settings.background_color }};
    color: {{ section.settings.text_color }};
    padding: {{ section.settings.padding_top }}px 0 {{ section.settings.padding_bottom }}px;
    margin-top: {{ section.settings.margin_top }}px;
    font-family: 'Hiragino Sans', 'Hiragino Kaku Gothic ProN', 'Yu Gothic', 'Meiryo', sans-serif;
    border-top: {{ section.settings.border_width }}px solid {{ section.settings.border_color }};
  }

  .footer-navigation-wrapper {
    max-width: {{ section.settings.container_width }}px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .footer-columns {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: {{ section.settings.column_gap }}px;
    margin-bottom: {{ section.settings.section_gap }}px;
  }

  .footer-column {
    display: flex;
    flex-direction: column;
  }

  .footer-column-title {
    font-size: {{ section.settings.title_font_size }}px;
    font-weight: {{ section.settings.title_font_weight }};
    color: {{ section.settings.title_color }};
    margin-bottom: {{ section.settings.title_margin_bottom }}px;
    letter-spacing: {{ section.settings.title_letter_spacing }}px;
    text-transform: {{ section.settings.title_text_transform }};
    line-height: 1.2;
  }

  .footer-links {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .footer-link-item {
    margin-bottom: {{ section.settings.link_spacing }}px;
  }

  .footer-link {
    color: {{ section.settings.link_color }};
    text-decoration: none;
    font-size: {{ section.settings.link_font_size }}px;
    font-weight: {{ section.settings.link_font_weight }};
    line-height: 1.4;
    transition: color 0.3s ease;
    display: block;
  }

  .footer-link:hover {
    color: {{ section.settings.link_hover_color }};
  }

  /* Social Links */
  .social-links {
    display: flex;
    flex-direction: column;
    gap: {{ section.settings.social_spacing }}px;
  }

  .social-link {
    display: flex;
    align-items: center;
    gap: 8px;
    color: {{ section.settings.social_color }};
    text-decoration: none;
    font-size: {{ section.settings.social_font_size }}px;
    font-weight: {{ section.settings.social_font_weight }};
    transition: color 0.3s ease;
    line-height: 1.4;
  }

  .social-link:hover {
    color: {{ section.settings.social_hover_color }};
  }

  .social-link svg {
    width: {{ section.settings.social_icon_size }}px;
    height: {{ section.settings.social_icon_size }}px;
    flex-shrink: 0;
  }

  .social-text {
    flex: 1;
  }

  /* 版权信息 */
  .footer-copyright {
    text-align: center;
    padding-top: {{ section.settings.copyright_padding_top }}px;
    border-top: 1px solid {{ section.settings.copyright_border_color }};
  }

  .copyright-text {
    font-size: {{ section.settings.copyright_font_size }}px;
    color: {{ section.settings.copyright_color }};
    margin: 0;
    line-height: 1.4;
  }

  /* 响应式设计 */
  @media (max-width: 768px) {
    .footer-navigation-container {
      padding: {{ section.settings.padding_top | minus: 20 }}px 0 {{ section.settings.padding_bottom | minus: 20 }}px;
    }

    .footer-navigation-wrapper {
      padding: 0 15px;
    }

    .footer-columns {
      grid-template-columns: repeat(2, 1fr);
      gap: {{ section.settings.column_gap | minus: 10 }}px {{ section.settings.column_gap }}px;
    }

    .footer-column-title {
      font-size: {{ section.settings.title_font_size | minus: 2 }}px;
      margin-bottom: {{ section.settings.title_margin_bottom | minus: 5 }}px;
    }

    .footer-link {
      font-size: {{ section.settings.link_font_size | minus: 1 }}px;
    }

    .social-link {
      font-size: {{ section.settings.social_font_size | minus: 1 }}px;
    }

    .social-link svg {
      width: {{ section.settings.social_icon_size | minus: 2 }}px;
      height: {{ section.settings.social_icon_size | minus: 2 }}px;
    }
  }

  @media (max-width: 480px) {
    .footer-columns {
      grid-template-columns: 1fr;
      gap: {{ section.settings.column_gap }}px;
    }

    .footer-column {
      text-align: left;
    }

    .footer-column-title {
      font-size: {{ section.settings.title_font_size | minus: 3 }}px;
    }

    .footer-link {
      font-size: {{ section.settings.link_font_size | minus: 2 }}px;
    }

    .social-links {
      flex-direction: row;
      flex-wrap: wrap;
      gap: 15px;
    }

    .social-link {
      font-size: {{ section.settings.social_font_size | minus: 2 }}px;
    }

    .copyright-text {
      font-size: {{ section.settings.copyright_font_size | minus: 2 }}px;
    }
  }
</style>

{% schema %}
{
