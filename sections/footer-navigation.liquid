<!-- 底部导航栏 - 四列布局 -->
<footer class="footer-navigation-container">
  <div class="footer-navigation-wrapper">
    <div class="footer-columns">
      <!-- カスタマーサービス列 -->
      <div class="footer-column">
        <h3 class="footer-column-title">{{ section.settings.customer_service_title | default: 'カスタマーサービス' }}</h3>
        <ul class="footer-links">
          {%- for block in section.blocks -%}
            {%- if block.type == 'customer_service_link' -%}
              <li class="footer-link-item">
                <a href="{{ block.settings.link_url | default: '#' }}" class="footer-link">
                  {{ block.settings.link_text }}
                </a>
              </li>
            {%- endif -%}
          {%- endfor -%}
        </ul>
      </div>

      <!-- メゾン列 -->
      <div class="footer-column">
        <h3 class="footer-column-title">{{ section.settings.maison_title | default: 'メゾン' }}</h3>
        <ul class="footer-links">
          {%- for block in section.blocks -%}
            {%- if block.type == 'maison_link' -%}
              <li class="footer-link-item">
                <a href="{{ block.settings.link_url | default: '#' }}" class="footer-link">
                  {{ block.settings.link_text }}
                </a>
              </li>
            {%- endif -%}
          {%- endfor -%}
        </ul>
      </div>

      <!-- 規約列 -->
      <div class="footer-column">
        <h3 class="footer-column-title">{{ section.settings.terms_title | default: '規約' }}</h3>
        <ul class="footer-links">
          {%- for block in section.blocks -%}
            {%- if block.type == 'terms_link' -%}
              <li class="footer-link-item">
                <a href="{{ block.settings.link_url | default: '#' }}" class="footer-link">
                  {{ block.settings.link_text }}
                </a>
              </li>
            {%- endif -%}
          {%- endfor -%}
        </ul>
      </div>

      <!-- FOLLOW US列 -->
      <div class="footer-column">
        <h3 class="footer-column-title">{{ section.settings.follow_us_title | default: 'FOLLOW US' }}</h3>
        <div class="social-links">
          {%- for block in section.blocks -%}
            {%- if block.type == 'social_link' -%}
              <a href="{{ block.settings.social_url | default: '#' }}" class="social-link" target="_blank" rel="noopener noreferrer">
