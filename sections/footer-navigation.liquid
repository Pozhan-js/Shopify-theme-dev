    {
      "type": "range",
      "id": "padding_top",
      "min": 20,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "顶部内边距",
      "default": 60
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 20,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "底部内边距",
      "default": 40
    },
    {
      "type": "range",
      "id": "margin_top",
      "min": 0,
      "max": 100,
      "step": 10,
      "unit": "px",
      "label": "顶部外边距",
