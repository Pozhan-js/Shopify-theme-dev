    },
    {
      "type": "range",
      "id": "border_width",
      "min": 0,
      "max": 5,
      "step": 1,
      "unit": "px",
      "label": "顶部边框宽度",
      "default": 1
    },
    {
      "type": "color",
      "id": "border_color",
      "label": "顶部边框颜色",
      "default": "#e0e0e0"
    },
    {
      "type": "header",
      "content": "列标题设置"
